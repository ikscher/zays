<?xml version="1.0" encoding="UTF-8"?>
<root>
<config_name>从旧表web_members_op,web_memberfield_op中导入到新表web_members_base</config_name>
<table>
	<name>web_members_base</name><!-- 要插入数据的表 -->
	<pretable>web_members_op,web_memberfield_op</pretable><!-- 数据来源的表 -->
	<step>2000</step><!-- 一次插入多少条 -->
	<fk>web_members_op.uid=web_memberfield_op.uid</fk><!-- 来源表关联 -->
